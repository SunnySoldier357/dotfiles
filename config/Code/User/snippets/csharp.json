{
    "Console WriteLine": {
        "prefix": "cw",
        "body": "Console.WriteLine($1);$0",
        "description": "Write line to Console"
    },
    "Debug WriteLine": {
        "prefix": "dw",
        "body": "Debug.WriteLine(\"${1:string}\");$2",
        "description": "Write line to Debug output"
    },
    "Debug WriteLine Error Message": {
        "prefix": "dwe",
        "body": "Debug.WriteLine(\"Error! \" + ${1:e}.Message);$2",
        "description": "Write error message to Debug output"
    },
    "Else if": {
        "prefix": "elif",
        "body": [
            "else if ($1)",
            "    $0"
        ]
    }
}